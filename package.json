{
  "name": "calendariale",
  "version": "0.8.0",
  "description": "Calendrical calculations library in TypeScript",
  "author": "catull@gmail.com",
  "license": "BSD-2-Clause",
  "keywords": [
    "calendar",
    "calendrcal calculations"
  ],
  "repository": {
    "type": "git",
    "url": "git+htts://github.com/catull/calendariale.git"
  },
  "bugs": {
    "url": "htts://github.com/catull/calendariale/issues"
  },
  "homepage": "htts://github.com/catull/calendariale",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build:check": "tsc -p .",
    "build": "tsup src/*.ts src/calendar/core/*.ts src/calendar/*.ts",
    "prebuild": "barro .barro.config.cjs --write",
    "prebuild.old": "barrelsby -D -H -q -l all -d src -e src/spec -e create-dist",
    "postinstall": "pnpm prebuild",
    "clean": "rimraf -fr dist tmp",
    "clean:real": "rimraf -fr dist tmp \"src/**/index.ts\"",
    "check-exports": "attw --pack --profile strict --profile esm-only --ignore-rules=cjs-resolves-to-esm --ignore-rules=unexpected-module-syntax dist/",
    "dist": "pnpm prebuild && pnpm lint && pnpm build:check && pnpm build && pnpm test:prod && pnpm dist:package.json && pnpm format:dist",
    "dist:package.json": "node create-dist-package.json.ts $npm_package_version > dist/package.json",
    "test": "vitest --run",
    "test:dev": "vitest --watch",
    "test:prod": "vitest --run --silent --coverage",
    "format": "biome format --fix --log-level=warn 'src'",
    "format:check": "biome format 'src'",
    "format:dist": "biome format --fix --log-level=warn 'dist'",
    "lint": "biome lint --fix 'src'",
    "lint:check": "biome lint 'src'"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.3",
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "2.27.12",
    "@swc/core": "1.10.16",
    "@types/node": "22.13.4",
    "@vitest/coverage-v8": "3.0.5",
    "barro": "1.2.8",
    "globals": "15.15.0",
    "happy-dom": "17.1.0",
    "pnpm": "10.4.0",
    "rimraf": "6.0.1",
    "tsup": "8.3.6",
    "typescript": "5.7.3",
    "vite": "6.1.0",
    "vitest": "3.0.5"
  },
  "packageManager": "pnpm@10.4.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@swc/core",
      "esbuild"
    ]
  }
}
